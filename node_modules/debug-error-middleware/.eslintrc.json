{
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "strict": 0,
        "prefer-rest-params": 0,
        "prettier/prettier": [
            "error",
            {
                "printWidth": 80,
                "trailingComma": "object",
                "singleQuote": true
            }
        ]
    }   
}